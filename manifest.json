{
  "manifest_version": 2,
  "name": "My First Extension",
  "version": "1.0",
  "description": "A simple extension to get started.",
  "browser_action": {
    "default_title": "My First Extension",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "<all_urls>"
  ]
}
